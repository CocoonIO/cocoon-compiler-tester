<?xml version="1.0" encoding="utf-8"?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:cocoon="http://cocoon.io/ns/1.0" xmlns:cdv="http://cordova.apache.org/ns/1.0" id="io.cocoon.test.webviewplus" version="1.0.8">
    <name>2.1.0 - Webviewplus</name>
    <description>Webviewplus</description>
    <author email="support@cocoon.io" href="https://cocoon.io">Ludei</author>
    <content src="index.html"/>
    <plugin name="cordova-plugin-whitelist"/>
    <access origin="*"/>
    <allow-intent href="http://*/*"/>
    <allow-intent href="https://*/*"/>
    <allow-intent href="tel:*"/>
    <allow-intent href="sms:*"/>
    <allow-intent href="mailto:*"/>
    <allow-intent href="geo:*"/>
    <preference name="Orientation" value="portrait"/>
    <preference name="Fullscreen" value="true"/>
    <preference name="windows-target-version" value="10.0"/>
    <engine name="android"/>
    <engine name="ios"/>
    <engine name="osx"/>
    <engine name="ubuntu"/>
    <engine name="windows"/>
    <!--<plugin name="cordova-plugin-wkwebview-engine" spec="~1.0.1" />
	<plugin name="cordova-labs-local-webserver" spec="~2.3.1" />
	<plugin name="cordova-labs-wkwebview-engine-localhost" spec="~0.4.0" />
			-->
    <plugin name="com.ludei.webviewplus.ios"/>
    <plugin name="com.ludei.webviewplus.android"/>
    <platform name="ios">
        <preference name="enabled" value="true"/>
    </platform>
    <platform name="android">
        <preference name="enabled" value="true"/>
    </platform>
    <preference name="cocoon-version" value="2.1.0"/>
</widget>
