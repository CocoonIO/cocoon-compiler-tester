	<plugin name="cocoon-plugin-ads-ios-mopub-chartboost" spec=""/>
	<plugin name="cocoon-plugin-ads-ios-mopub-millennial" spec=""/>
	<plugin name="cocoon-plugin-ads-ios-mopub-adcolony" spec=""/>
	<preference name="CocoonSplashDelay" value="3"/>
