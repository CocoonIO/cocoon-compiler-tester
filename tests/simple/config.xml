<?xml version="1.0" encoding="utf-8"?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:cocoon="http://cocoon.io/ns/1.0" xmlns:cdv="http://cordova.apache.org/ns/1.0" id="io.cocoon.test.simple" version="1.0.8">
    <name>2.1.0 - Simple</name>
    <description>Simple</description>
    <author email="support@cocoon.io" href="https://cocoon.io">Ludei</author>
    <content src="index.html"/>
    <plugin name="cordova-plugin-whitelist"/>
    <access origin="*"/>
    <allow-intent href="http://*/*"/>
    <allow-intent href="https://*/*"/>
    <allow-intent href="tel:*"/>
    <allow-intent href="sms:*"/>
    <allow-intent href="mailto:*"/>
    <allow-intent href="geo:*"/>
    <preference name="Orientation" value="portrait"/>
    <preference name="Fullscreen" value="true"/>
    <preference name="windows-target-version" value="10.0"/>
    <engine name="android"/>
    <engine name="ios"/>
    <engine name="osx"/>
    <engine name="ubuntu"/>
    <engine name="windows"/>
    <plugin name="com.ludei.canvasplus.ios"/>
    <plugin name="com.ludei.canvasplus.android"/>
    <platform name="ios">
        <preference name="enabled" value="true"/>
    </platform>
    <platform name="android">
        <preference name="enabled" value="true"/>
    </platform>
    <platform name="windows">
        <preference name="enabled" value="true"/>
    </platform>
    <platform name="osx">
        <preference name="enabled" value="true"/>
    </platform>
    <platform name="ubuntu">
        <preference name="enabled" value="true"/>
    </platform>
    <preference name="cocoon-version" value="2.1.0"/>
</widget>
